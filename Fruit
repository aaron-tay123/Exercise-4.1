package Exercise;

public class Fruit {
	private String name;
	
	public Fruit (String name) {
		this.name = name;
	System.out.println("Fruit constructor is invoked");
	}
	
	public String getName() {
		return name;
	}
	
	public String toString() {
		return "Eating more fruit is very healthy for our body";
	}
}
